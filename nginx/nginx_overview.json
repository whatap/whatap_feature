            "name": "oname",
            "mql": "INJECT timepast\nHEADER { \"response_2xx$\":\"#\" }\nOIDSET { oid:$oid, okind:$okind, onode:$onode }\nCATEGORY {\"telegraf_nginxlog\":6h, \"telegraf_nginxlog{m5}\":3d, \"telegraf_nginxlog{h1}\":unlimit } \nTAGLOAD\nINJECT default\n\nUPDATE { key: ['response_200','response_201','response_202','response_204'], value: $TIME_MERGE_PLACE }\nSELECT [ 'pcode', 'time', 'oid', 'response_200','response_201','response_202','response_204', 'oname' ]\nFILTER { key: \"oname\", exist: true }\n\nCREATE { key:_id_, expr: \"pcode+'_'+oname\" }\nCREATE { key:_name_, expr: \"oname\" }\nupdate { key:response_200, notnull:0 }\nupdate { key:response_201, notnull:0 }\nupdate { key:response_202, notnull:0 }\nupdate { key:response_204, notnull:0 }\nCREATE { key:response_2xx, expr: \"response_200+response_201+response_202+response_204\"}\nGROUP { timeunit: 30s, merge: ['response_2xx'], pk: _id_, last: _name_ }\nUPDATE { key: ['response_2xx'], value: $OBJECT_MERGE_PLACE }\nFILTER { key: \"response_2xx\", exist: true }"
